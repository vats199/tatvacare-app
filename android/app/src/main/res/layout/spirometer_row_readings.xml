<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginHorizontal="@dimen/screen_space"
    android:layout_marginTop="@dimen/dp_6"
    android:layout_marginBottom="@dimen/dp_6"
    app:layout_constraintBottom_toBottomOf="parent"
    android:background="@drawable/bg_common_rounded_rect_10"
    android:elevation="@dimen/dp_2"
    android:paddingBottom="@dimen/dp_12">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textViewLabelStandardTest"
        style="@style/TextView.SemiBold.sp_14"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_16"
        android:layout_marginTop="@dimen/dp_14"
        android:text="Standard Test"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@id/imageViewDownload"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textViewLabelDateAndTime"
        style="@style/TextView.Regular.sp_12"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:ellipsize="end"
        android:maxLines="1"
        android:text="Date &amp; Time"
        android:textColor="@color/gray2"
        app:layout_constraintEnd_toStartOf="@id/imageViewDownload"
        app:layout_constraintStart_toStartOf="@+id/textViewLabelStandardTest"
        app:layout_constraintTop_toBottomOf="@id/textViewLabelStandardTest" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageViewDownload"
        android:layout_width="@dimen/dp_32"
        android:layout_height="@dimen/dp_32"
        android:layout_marginEnd="@dimen/dp_14"
        android:background="?selectableItemBackgroundBorderless"
        android:padding="@dimen/dp_6"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@id/textViewLabelDateAndTime"
        app:layout_constraintDimensionRatio="1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/textViewLabelStandardTest"
        app:srcCompat="@drawable/ic_downloads" />

    <View
        android:id="@+id/viewLine"
        style="@style/viewLine"
        android:layout_width="0dp"
        android:layout_marginHorizontal="@dimen/dp_16"
        android:layout_marginTop="@dimen/dp_13"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textViewLabelDateAndTime" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layoutStandardTestReading"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/dp_16"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginBottom="@dimen/dp_5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/viewLine"
        app:layout_constraintVertical_bias="0">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textViewValueFvc"
            style="@style/TextView.SemiBold.sp_16"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_12"
            android:ellipsize="end"
            android:maxLines="1"
            tools:text="3.07L"
            android:textColor="@color/black"
            app:layout_constraintEnd_toStartOf="@id/textViewValueFev1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textViewValuePercentFvc"
            style="@style/TextView.Light.sp_12"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_6"
            android:ellipsize="end"
            android:maxLines="1"
            tools:text="70.00% (P)"
            android:textColor="@color/gray2"
            app:layout_constraintEnd_toEndOf="@+id/textViewValueFvc"
            app:layout_constraintStart_toStartOf="@+id/textViewValueFvc"
            app:layout_constraintTop_toBottomOf="@+id/textViewValueFvc" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textViewFvc"
            style="@style/TextView.Medium.sp_14"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_6"
            android:ellipsize="end"
            android:maxLines="1"
            android:text="FVC"
            android:textColor="@color/gray2"
            app:layout_constraintEnd_toEndOf="@+id/textViewValuePercentFvc"
            app:layout_constraintStart_toStartOf="@+id/textViewValuePercentFvc"
            app:layout_constraintTop_toBottomOf="@+id/textViewValuePercentFvc" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textViewValueFev1"
            style="@style/TextView.SemiBold.sp_16"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_10"
            android:ellipsize="end"
            android:maxLines="1"
            tools:text="2.20L"
            android:textColor="@color/black"
            app:layout_constraintBottom_toBottomOf="@id/textViewValueFvc"
            app:layout_constraintEnd_toStartOf="@id/textViewValueRatio"
            app:layout_constraintStart_toEndOf="@id/textViewValueFvc"
            app:layout_constraintTop_toTopOf="@id/textViewValueFvc" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textViewValuePercentFev1"
            style="@style/TextView.Light.sp_12"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_6"
            android:ellipsize="end"
            android:maxLines="1"
            tools:text="70.00% (P)"
            android:textColor="@color/gray2"
            app:layout_constraintEnd_toEndOf="@+id/textViewValueFev1"
            app:layout_constraintStart_toStartOf="@+id/textViewValueFev1"
            app:layout_constraintTop_toBottomOf="@+id/textViewValueFev1" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textViewFev1"
            style="@style/TextView.Medium.sp_14"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_6"
            android:ellipsize="end"
            android:maxLines="1"
            android:text="FEV1"
            android:textColor="@color/gray2"
            app:layout_constraintEnd_toEndOf="@+id/textViewValuePercentFev1"
            app:layout_constraintStart_toStartOf="@+id/textViewValuePercentFev1"
            app:layout_constraintTop_toBottomOf="@+id/textViewValuePercentFev1" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textViewValueRatio"
            style="@style/TextView.SemiBold.sp_16"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_10"
            android:ellipsize="end"
            android:maxLines="1"
            tools:text="72.0%"
            android:textColor="@color/black"
            app:layout_constraintBottom_toBottomOf="@id/textViewValueFev1"
            app:layout_constraintEnd_toStartOf="@id/textViewValuePef"
            app:layout_constraintStart_toEndOf="@id/textViewValueFev1"
            app:layout_constraintTop_toTopOf="@id/textViewValueFev1" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textViewValuePercentRatio"
            style="@style/TextView.Light.sp_12"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_6"
            android:ellipsize="end"
            android:maxLines="1"
            tools:text="70.00% (P)"
            android:textColor="@color/gray2"
            app:layout_constraintEnd_toEndOf="@+id/textViewValueRatio"
            app:layout_constraintStart_toStartOf="@+id/textViewValueRatio"
            app:layout_constraintTop_toBottomOf="@+id/textViewValueRatio" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textViewRatio"
            style="@style/TextView.Medium.sp_14"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_6"
            android:ellipsize="end"
            android:maxLines="1"
            android:text="Ratio"
            android:textColor="@color/gray2"
            app:layout_constraintEnd_toEndOf="@+id/textViewValuePercentRatio"
            app:layout_constraintStart_toStartOf="@+id/textViewValuePercentRatio"
            app:layout_constraintTop_toBottomOf="@+id/textViewValuePercentRatio" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textViewValuePef"
            style="@style/TextView.SemiBold.sp_16"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_10"
            android:ellipsize="end"
            android:maxLines="1"
            tools:text="233 L/M"
            android:textColor="@color/black"
            app:layout_constraintBottom_toBottomOf="@id/textViewValueRatio"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/textViewValueRatio"
            app:layout_constraintTop_toTopOf="@id/textViewValueRatio" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textViewValuePercentPef"
            style="@style/TextView.Light.sp_12"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_6"
            android:ellipsize="end"
            android:maxLines="1"
            tools:text="70.00% (P)"
            android:textColor="@color/gray2"
            app:layout_constraintEnd_toEndOf="@+id/textViewValuePef"
            app:layout_constraintStart_toStartOf="@+id/textViewValuePef"
            app:layout_constraintTop_toBottomOf="@+id/textViewValuePef" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textViewPef"
            style="@style/TextView.Medium.sp_14"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_6"
            android:ellipsize="end"
            android:maxLines="1"
            android:text="PEF"
            android:textColor="@color/gray2"
            app:layout_constraintEnd_toEndOf="@+id/textViewValuePercentPef"
            app:layout_constraintStart_toStartOf="@+id/textViewValuePercentPef"
            app:layout_constraintTop_toBottomOf="@+id/textViewValuePercentPef" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layoutIncentiveTestReading"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/dp_16"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginBottom="@dimen/dp_5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/layoutStandardTestReading">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textViewValueMeasuredVolume"
            style="@style/TextView.SemiBold.sp_16"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_12"
            android:ellipsize="end"
            android:maxLines="1"
            tools:text="2.20L"
            android:textColor="@color/black"
            app:layout_constraintEnd_toStartOf="@id/textViewValueTargetVolume"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textViewValueTargetVolume"
            style="@style/TextView.SemiBold.sp_16"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_20"
            android:ellipsize="end"
            android:maxLines="1"
            tools:text="2.20L"
            android:textColor="@color/black"
            app:layout_constraintBottom_toBottomOf="@id/textViewValueMeasuredVolume"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/textViewValueMeasuredVolume"
            app:layout_constraintTop_toTopOf="@id/textViewValueMeasuredVolume" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textViewTargetVolume"
            style="@style/TextView.Medium.sp_14"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_6"
            android:ellipsize="end"
            android:maxLines="1"
            android:text="Target Volume"
            android:textColor="@color/gray2"
            app:layout_constraintEnd_toEndOf="@+id/textViewValueTargetVolume"
            app:layout_constraintStart_toStartOf="@+id/textViewValueTargetVolume"
            app:layout_constraintTop_toBottomOf="@+id/textViewValueTargetVolume" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textViewMeasuredVolume"
            style="@style/TextView.Medium.sp_14"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_6"
            android:ellipsize="end"
            android:maxLines="1"
            android:text="Measured  Volume"
            android:textColor="@color/gray2"
            app:layout_constraintEnd_toEndOf="@+id/textViewValueMeasuredVolume"
            app:layout_constraintStart_toStartOf="@+id/textViewValueMeasuredVolume"
            app:layout_constraintTop_toBottomOf="@+id/textViewValueMeasuredVolume" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>