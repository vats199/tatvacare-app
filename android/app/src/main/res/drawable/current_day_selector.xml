<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true">
        <shape android:innerRadius="0dp" android:innerRadiusRatio="700" android:shape="ring" android:thickness="@dimen/dp_16" android:useLevel="false">
            <solid android:color="@color/textBlack1" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:innerRadius="0dp" android:innerRadiusRatio="700" android:shape="ring" android:thickness="@dimen/dp_16" android:useLevel="false">
            <solid android:color="@color/textBlack1" />
        </shape>
    </item>

    <item>
        <shape android:innerRadius="0dp" android:innerRadiusRatio="700" android:shape="ring" android:thickness="@dimen/dp_16" android:useLevel="false">
            <solid android:color="@color/textBlack1" />
        </shape>
    </item>

</selector>
