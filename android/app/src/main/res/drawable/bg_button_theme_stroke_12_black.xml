<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:color="#50ffffff">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
            <stroke android:width="@dimen/dp_1" android:color="@color/gray3" />
            <corners android:radius="@dimen/dp_12" />
        </shape>
    </item>
    <item>
        <ripple android:color="@color/textBlack9">
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="@color/white" />
                    <stroke android:width="@dimen/dp_1" android:color="@color/textBlack9" />
                    <corners android:radius="@dimen/dp_12" />
                </shape>
            </item>
        </ripple>
    </item>
</selector>